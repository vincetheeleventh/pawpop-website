# Test environment variables for Playwright E2E tests
# Copy this to .env.test and fill in your real values
# The .env.test file is gitignored for security

# Base URL for testing
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Real Supabase configuration (required for E2E tests)
# Copy these from your .env.local file
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_real_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_real_service_role_key_here

# Mock values for other services (E2E tests mock these APIs)
STRIPE_PUBLISHABLE_KEY=pk_test_mock_publishable_key
STRIPE_SECRET_KEY=sk_test_mock_secret_key
STRIPE_WEBHOOK_SECRET=whsec_mock_webhook_secret
FAL_API_KEY=mock_fal_api_key
UPLOADTHING_SECRET=mock_uploadthing_secret
UPLOADTHING_APP_ID=mock_uploadthing_app_id
PRINTIFY_API_TOKEN=mock_printify_token
PRINTIFY_SHOP_ID=mock_shop_id
PRINTIFY_WEBHOOK_SECRET=mock_printify_webhook_secret
